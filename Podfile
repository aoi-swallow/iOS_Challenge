  target 'iOS_ChallengeTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Moya'
    pod 'RxSwift'
    pod 'RxCocoa'
    pod 'Moya/RxSwift'
    pod 'RealmSwift'
  end

  target 'iOS_ChallengeUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
